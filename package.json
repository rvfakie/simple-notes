{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "node ./node_modules/eslint/bin/eslint.js --format gitlab --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lintfix": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc.cjs --fix --ext .js,.vue,.ts --ignore-path .gitignore .",
    "typecheck": "nuxi typecheck",
    "stylelint": "node ./node_modules/stylelint/bin/stylelint.mjs \"**/*.{scss,vue}\"",
    "stylelintfix": "node ./node_modules/stylelint/bin/stylelint.mjs \"**/*.{scss,vue}\" --fix",
    "init-husky": "husky install ./husky",
    "prepare": "husky install ./husky",
    "lint-staged": "lint-staged -c ./husky/lint-staged-config.js"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.3",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@stylistic/eslint-plugin-js": "^2.3.0",
    "@stylistic/eslint-plugin-ts": "^2.6.0",
    "@stylistic/stylelint-plugin": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-formatter-gitlab": "^4.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^7.0.1",
    "lint-staged": "^16.1.4",
    "sass": "^1.89.2",
    "stylelint": "^16.23.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "vue-tsc": "^3.0.5"
  }
}
